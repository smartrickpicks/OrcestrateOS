{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Opportunity Field Catalog Bundle",
  "type": "object",
  "required": [
    "version",
    "entries"
  ],
  "properties": {
    "version": {
      "type": "string"
    },
    "meta": {
      "type": "object"
    },
    "entries": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [
          "field_key"
        ],
        "properties": {
          "field_key": {
            "type": "string"
          },
          "tier": {
            "type": "string"
          },
          "is_hinge": {
            "type": "string"
          },
          "parent_group": {
            "type": "string"
          },
          "data_type_guess": {
            "type": "string"
          },
          "notes": {
            "type": "string"
          }
        },
        "additionalProperties": false
      }
    }
  },
  "additionalProperties": false
}